            <th>
                @Html.DisplayNameFor(model => model.sonDuzenlemeTarih)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Kullanici2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Kullanici3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Yetki)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.soyad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.kullaniciAd)
