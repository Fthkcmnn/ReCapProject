                @Html.DisplayFor(modelItem => item.silindiMi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.olusturmaTarih)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dogumTarih)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.sonDuzenlemeTarih)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Kullanici2.ad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Kullanici3.ad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Yetki.ad)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.kullaniciID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.kullaniciID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.kullaniciID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
