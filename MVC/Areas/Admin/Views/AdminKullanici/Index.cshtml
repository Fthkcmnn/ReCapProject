@model IEnumerable<Entities.Concrete.Kullanici>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.soyad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.kullaniciAd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.sifre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ePosta)
